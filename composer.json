{
    "name": "endroid/pdf",
    "description": "Endroid PDF",
    "keywords": ["endroid", "pdf"],
    "homepage": "https://github.com/endroid/pdf",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "knplabs/knp-snappy": "^1.0",
        "iio/libmergepdf": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Pdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Pdf\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
